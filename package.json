    "build": "ts-node build.ts && tsc",
    "clear": "ts-node build.ts",
    "pulldb": "ts-node pulldb.ts",
    "update:xapi": "git fetch upstream && git pull upstream master && npm install",
    "update:submodule": "git submodule update --init --recursive --force â€“remote",
    "update:prisma": "npm i --save-dev prisma@latest && npm i @prisma/client@latest",
    "git:push": "git add * && git commit -m \"update\" && git push origin master",
    "git:reset": "git fetch origin && git reset --hard origin/master",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
